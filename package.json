{
  "name": "native-proxy-agent",
  "version": "2.5.0",
  "description": "A native proxy agent that uses the native nodejs http.Agent and https.Agent to proxy the requests, no dependencies",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean",
    "test": "jest",
    "release": "standard-version",
    "lint": "eslint --ext .ts src",
    "cm": "git-cz",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "registry": "https://axags.jfrog.io/axags/api/npm/lib-amf-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.axa.com/Digital/native-proxy-agent.git"
  },
  "keywords": [
    "agent",
    "proxy",
    "keep-alive",
    "http-proxy",
    "https-proxy",
    "AXA"
  ],
  "author": "AXA GO Digital",
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "8.3.4",
    "@types/jest": "25.1.2",
    "@types/node": "13.7.1",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "commitizen": "4.0.3",
    "commitlint": "8.3.5",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-prettier": "3.1.2",
    "got": "10.5.5",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "nock": "11.8.2",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "standard-version": "7.1.0",
    "ts-jest": "25.2.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "hyperid": "2.0.3",
    "node-object-hash": "2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
